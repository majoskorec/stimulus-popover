    "@hotwired/stimulus": "^3.1.0",
    "np": "^7.6.2",
    "prettier-standard": "16.4.1",
    "typescript": "^4.7.4",
    "vite": "^2.3.6"
  },
  "peerDependencies": {
    "@hotwired/stimulus": "^3.1.0"
